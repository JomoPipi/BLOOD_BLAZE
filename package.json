{
  "name": "blood_blaze",
  "version": "1.0.0",
  "description": "DEVASTATING BLOOD DEATH EXTREME",
  "type": "module",
  "scripts": {
    "test": "jest",
    "dev-client": "cd src/client && npx rollup -c -w",
    "dev-server": "concurrently npm:dev-server:*",
    "dev-server:compile": "tsc -w",
    "dev-server:run": "nodemon"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      ".git",
      "node_modules/",
      "dist/",
      "coverage/"
    ],
    "watch": [
      "src/"
    ],
    "exec": "node --loader ts-node/esm src/server/main.ts",
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "js,json,ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JomoPipi/BLOOD_BLAZE.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JomoPipi/BLOOD_BLAZE/issues"
  },
  "homepage": "https://github.com/JomoPipi/BLOOD_BLAZE#readme",
  "dependencies": {
    "express": "^4.17.1",
    "socket.io": "^4.1.2"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.14.5",
    "@tsconfig/svelte": "^2.0.1",
    "@types/express": "^4.17.12",
    "@types/node": "^15.12.2",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "concurrently": "^6.2.0",
    "jest": "^27.0.4",
    "nodemon": "^2.0.7",
    "sirv-cli": "^1.0.12",
    "svelte-check": "^2.1.0",
    "svelte-preprocess": "^4.7.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  }
}
